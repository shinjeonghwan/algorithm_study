#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>


//투포인터로 푸는데 포인터 사이의 값들이 M 넘어가면 왼쪽 포인터 전진, 사이 합이 M 안넘어 가면 오른쪽 포인터 전진.


int compare(const void *a, const void *b)
{
    int num1 = *(int *)a;
    int num2 = *(int *)b;


    if (num1>num2)
    {
        return 1;
    }
    else if(num1<num2)
    {
        return -1;
    }
    else
        return 0;

}

int main()
{
    int N = 0;

    int cnt = 0;
    int tmp = 0;

    int sum = 0;

    int div = 0;
    int mid = 0;
    int mode = 0;
    int range = 0;

    int freq = 0;
    int cnt = 0;


    scanf("%d",&N);

    int numbers[500000] = {0,};
    int tmp_numbers[500000] = {0,};


    for(int i = 0; i < N; i++)
    {
        scanf("%d", &tmp);
        numbers[i] = tmp;
    }

    qsort(numbers, N , sizeof(numbers[0]), compare);


    for(int i = 0; i < N; i++)
    {
        sum += numbers[i];
    }

    for (int i = 0; i < N; i++)
    {
        freq = 1;
        for (int j = i+1; j < N; j++)
        {
            if(numbers[i] == numbers[j])
            {
                    freq++;
            }
        }
        if

    }




    div = sum/N;
    mid = numbers[N/2];
    //mode =
    range = numbers[N-1] - numbers[0];


    printf("%d\n%d\n%d\n%d\n", div, mid, mode, range);

    return 0;
}





