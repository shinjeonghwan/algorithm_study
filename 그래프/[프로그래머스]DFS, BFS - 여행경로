import collections

def solution(tickets):
    answer = []
    
    graph = collections.defaultdict(list)
    
    for a,b in sorted(tickets):
        graph[a].append(b)
    
    def dfs(a):
        while graph[a]:
            dfs(graph[a].pop(0))
        answer.append(a)
    
    dfs('ICN')
    
    print(answer[::-1])
    return answer[::-1]
