#define _CRT_SECURE_NO_WARNINGS
#include <iostream>
#include <algorithm>
#include <vector>

using namespace std;

int N;
int Team_Max_num;
int S[21][21];
int start_sum;
int link_sum;
int answer = 1000001;
bool Team[21]; // true는 start팀, false는 link팀

void bt(int Cur, int Cnt) {   //Cnt는 현재까지 선택된 사람 수.
	if (Cnt == Team_Max_num) {
		start_sum = 0;
		link_sum = 0;
		for (int i = 1; i <= N; i++) {
			for (int j = 1; j <= N; j++) {
				if (i == j) continue;

				if (Team[i] == true && Team[j] == true) { // 둘 다 start 팀일 때,
					start_sum += S[i][j];
				}
				else if (Team[i] == false && Team[j] == false) {//둘 다 link 팀일 때,
					link_sum += S[i][j];
				}
			}
		}
		answer = min(answer, abs(start_sum - link_sum));
		return;
	}

	else {
		for (int i = Cur; i <= N; i++) {
			if (Team[i] == false) {
				Team[i] = true;
				bt(i+1, Cnt + 1);
				Team[i] = false;
			}
		}

	}

}



int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);
	cin >> N;
	Team_Max_num = N / 2;

	for (int i = 1; i <= N; i++) {
		for (int j = 1; j <= N; j++) {
			cin >> S[i][j];
		}
	}

	bt(1, 0);

	cout << answer;

	return 0;
}
